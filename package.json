{
  "name": "techrv-fe",
  "version": "0.0.3",
  "private": true,
  "scripts": {
    "postinstall": "npx patch-package",
    "dev": "next dev",
    "build": "next build",
    "start": "node .next/standalone/server.js",
    "lint:ts": "tsc --noEmit",
    "lint": "next lint && yarn lint:ts",
    "prettier-update": "prettier . --write",
    "codegen:download": "curl -o ./src/common/openapi/swagger.json -u ntvinhit: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 -LJO https://raw.githubusercontent.com/techrv/techrv-be/dev/app/interface/api/docs/swagger.json",
    "codegen:local": "openapi --input ./src/common/openapi/swagger.json --output ./src/common/openapi/ --client=axios --indent=2 --name=AxiosClient",
    "codegen": "yarn codegen:download && yarn codegen:local",
    "docker-dev-build": "docker build --progress=plain -t techrv-fe:dev-$(node -p \"require('./package.json').version\") .",
    "docker-prod-build": "docker build -t techrv-fe:$(node -p \"require('./package.json').version\") .",
    "git-auto-dev-tag": "git tag dev-v$(node -p \"require('./package.json').version\") --force",
    "git-push-tags": "git push origin dev-v$(node -p \"require('./package.json').version\") --force",
    "git-force-update": "yarn git-auto-dev-tag -f && yarn git-push-tags -f",
    "gen-paths": "./tools/nextjs-typescript-path-generator.js"
  },
